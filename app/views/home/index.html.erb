<%= map(:center => {
    :latlng => @map.center,
    :zoom => @map.zoom
  },
  :markers => @map.establishments.map do |establishment|
    {
      :latlng => establishment.coordinates,
      :icon => { icon_url: asset_path("marker-icon") },
      :popup => render("establishment_marker_popup", establishment: establishment)
    }
  end
)
%>
